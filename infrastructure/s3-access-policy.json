{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "MemoryFinderS3FullAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "s3:GetBucketVersioning"
      ],
      "Resource": [
        "arn:aws:s3:::memory-finder-raw-120915929747-us-east-2",
        "arn:aws:s3:::memory-finder-raw-120915929747-us-east-2/*",
        "arn:aws:s3:::memory-finder-processed-120915929747-us-east-2",
        "arn:aws:s3:::memory-finder-processed-120915929747-us-east-2/*"
      ]
    },
    {
      "Sid": "S3PresignedUrlGeneration",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::memory-finder-raw-120915929747-us-east-2/*",
        "arn:aws:s3:::memory-finder-processed-120915929747-us-east-2/*"
      ]
    }
  ]
}
